{
    "name": "liip/yellowcube",
    "authors": [
        {
            "name": "Sylvain Raye",
            "email": "sly@diglin.com"
        }
    ],
    "require":{
        "php": ">=5.3.2",
        "magento-hackathon/magento-composer-installer":"2.*",        
    	"liip/yellowcube-php": "dev-master"
    },
    "repositories" : [
		{
            "type" : "composer",
            "url" : "http://packages.firegento.com"
        },
        {
        	"type": "vcs",
        	"url": "https://github.com/liip/yellowcube-php.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/liip-forks/wse-php.git"
        }

    ],
    "extra" : {
        "magento-root-dir" : "./",
        "magento-deploystrategy": "link",
        "magento-force": true
    },
    "config": {
    	"preferred-install": "dist"
    },
    "scripts": {
        "post-package-install": [
            "YellowCube\\Composer\\Magento::postPackageAction"
        ],
        "post-package-update": [
            "YellowCube\\Composer\\Magento::postPackageAction"
        ],
        "pre-package-uninstall": [
            "YellowCube\\Composer\\Magento::cleanPackageAction"
        ]
    },
    "minimum-stability": "dev"
}
